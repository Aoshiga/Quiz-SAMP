<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizsamp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizSAMP">
        <activity android:name="quizSAMPApp.vue.EditActivity"></activity>
        <activity android:name="quizSAMPApp.vue.ScoreActivity" />
        <activity android:name="quizSAMPApp.vue.QuestionActivity" />
        <activity android:name="quizSAMPApp.vue.ThemeActivity" />
        <activity android:name="quizSAMPApp.vue.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>